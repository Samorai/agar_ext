{
    "manifest_version": 2,
    "name": "Agar Time",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "injectedCode.js"
            ],
            "run_at": "document_start"
        }
    ],
    "browser_action": {
        "default_title": "AgarTime",
        "default_popup": "popup.html"
    }
}
