{
    "manifest_version": 2,
    "name": "Agar Time",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "jquery.min.js",
                "injectedCode.js"
            ]
        }
    ],
    "browser_action": {
        "default_title": "AgarTime",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"],
    "web_accessible_resources": [
        "client.js"
    ]
}
